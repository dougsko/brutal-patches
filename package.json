{
  "name": "brutal-patches",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --browsers=ChromeHeadless",
    "start:all": "npm --prefix server-src run start:dev & npm run start",
    "deploy": "bash scripts/deploy.sh",
    "deploy:backend": "cd server-src && npm run deploy",
    "deploy:frontend": "npm run build:prod && aws s3 sync dist/ s3://$S3_BUCKET_NAME --delete"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "angular-awesome-slider": "^2.4.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.0",
    "jquery-knob": "^1.2.11",
    "jquery-slider": "^0.0.1",
    "jquery-templates": "^1.0.0",
    "jquery-ui": "^1.13.0",
    "jqwidgets-ng": "^14.1.17",
    "jsplumb": "^2.15.6",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular/cli": "^18.2.20",
    "@angular/compiler-cli": "^18.2.13",
    "@jefiozie/ngx-aws-deploy": "^4.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.19.9",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.4.5"
  }
}
